	"dataSources": {
		"base": {
			"type": "ds.search",
			"options": {
				"query": "index=_internal sourcetype=splunkd_access | stats count",
				"queryParameters": {
					"earliest": "-1d",
					"latest": "now"
				}
			},
			"name": "base"
		},
		"post1": {
			"type": "ds.chain",
			"options": {
				"query": " eval count=count*1.2",
				"extend": "base"
			},
			"name": "post1"
		},
		"post2": {
			"type": "ds.chain",
			"options": {
				"query": " eval count=count/1.2",
				"extend": "base"
			},
			"name": "post2"
		},
		"post3": {
			"type": "ds.chain",
			"options": {
				"query": " eval count=count+100",
				"extend": "base"
			},
			"name": "post3"
		},
		"post4": {
			"type": "ds.chain",
			"options": {
				"query": " eval count=count-100",
				"extend": "base"
			},
			"name": "post4"
		},
